{
  "name": "mrap/graphcool",
  "description": "A GraphQL Microframework in PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Huber",
      "email": "michael.huber@hellocash.at"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Mrap\\GraphCool\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mrap\\GraphCool\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.0",
    "webonyx/graphql-php": "^14.4",
    "ext-json": "^8.0",
    "vlucas/phpdotenv": "^5.2",
    "ext-pdo": "^8.0",
    "ramsey/uuid": "^4.1",
    "box/spout": "^3.1",
    "mll-lab/graphql-php-scalars": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "test": "phpunit"
  }
}